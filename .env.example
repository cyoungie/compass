# Copy this file to .env and add your keys (do not commit .env)

# Anthropic (Claude): https://console.anthropic.com/
ANTHROPIC_API_KEY=your-api-key-here

# Google Maps (Resources tab: nearby food banks, shelters, FQHCs by zip)
# See GOOGLE_MAPS_SETUP.md for enabling Geocoding + Places API and creating a key
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=

# Firebase (optional - for auth, cloud profile, real community)
# Get these from Firebase Console > Project settings > General > Your apps
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID= 
EXPO_PUBLIC_FIREBASE_APP_ID=
